{
    "server": {
        "listen_ip": "0.0.0.0",
        "listen_port": 5555,
        "idle_timeout": 2000,
        "server_hostname": "test",
        "enable_https": false,
        "enable_mysql": false,
        "enable_php": true,
        "enable_simple_cgi": false,
        "default_page": [
            "index.php",
            "index.html"
        ]
    },
    "https": {
        "cert_file": "./ssl/cert.crt",
        "private_key_file": "./ssl/private.pem",
        "password": ""
    },
    "mysql": {
        "host": "127.0.0.1",
        "port": 3306,
        "username": "root",
        "password": "root",
        "database": "db",
        "table": "tb"
    },
    "cgi": {
        "cgi_bin": "cgi-bin"
    },
    "php-fpm": {
        "server_ip": "127.0.0.1",
        "server_port": 9000,
        "sock_path": "/run/php-fpm/php-fpm.sock"
    }
}